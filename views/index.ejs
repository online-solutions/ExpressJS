<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

  <% include templates/header.ejs %>

    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p>My name is <%= name %></p>
    <p><%= globalVariable %></p>

    <h3><%= videodata.categoryName %></h3>
    <ul>
        <% videodata.categories.forEach(function (item) {%>
            <li><%= item.categoryName %></li>
        <% }) %>

    </ul>

    <p>----------------</p>
    <ul>
      <% for(var i =0; i< 3; i++){ %>
        <li>
          Loop i = <%= i %>
        </li>

      <% } %>
    </ul>
  </body>
</html>
